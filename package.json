{
  "name": "WebUI-Aria2-Fork",
  "version": "1.0.0",
  "description": "Aria2 WebUI Fork",
  "homepage": "https://github.com/koumaza/webui-aria2-electron#readme",
  "author": {
    "name": "koumaza",
    "email": "koumaza--@outlook.com",
    "url": "https://koumaza.github.io"
  },
  "license": "MIT",
  "browserslist": [
    "> 5%",
    "IE 11",
    "last 2 versions"
  ],
  "scripts": {
    "dep": "npx pnpm i --no-lockfile",
    "dev": "webpack --mode development --watch --progress --stats-children & node node-server",
    "build": "webpack --mode=production --progress",
    "analyze": "webpack --mode=production --profile --json > stats.json && webpack-bundle-analyzer stats.json ./docs -p 9999",
    "format": "prettier -l --write  \"**/*{js,scss}\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koumaza/webui-aria2-electron.git"
  },
  "bugs": {
    "url": "https://github.com/ziahamza/webui-aria2-electron/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "angular": "latest",
    "angular-translate": "latest",
    "autoprefixer": "latest",
    "bootstrap-sass": "latest",
    "clean-webpack-plugin": "latest",
    "css-loader": "latest",
    "electron-builder": "next",
    "file-loader": "latest",
    "flag-icon-css": "latest",
    "html-webpack-plugin": "latest",
    "jquery": "beta",
    "lodash": "latest",
    "mini-css-extract-plugin": "latest",
    "sass": "latest",
    "postcss": "latest",
    "postcss-loader": "latest",
    "sass-loader": "latest",
    "webpack": "latest",
    "webpack-bundle-analyzer": "latest",
    "webpack-cli": "latest",
    "workbox-webpack-plugin": "latest"
  },
  "devDependencies": {
    "husky": "latest",
    "prettier": "latest",
    "pretty-quick": "latest"
  }
}
